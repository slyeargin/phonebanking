%html
  %head
    %title Remote Phonebanking
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => false
    = csrf_meta_tags
    = favicon_link_tag 'favicon.png'
